{
  "pipeline": {
    "build": {
      "outputs": ["dist/**", "build/**"],
      "dependsOn": ["^build"],
      "inputs": ["**/**/*.tsx", "**/**/*.ts"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "dependsOn": ["api#pre:dev"],
      "cache": false
    },
    "pre:dev": {
      "cache": false
    }
  }
}
