{
  "pipeline": {
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build", "db:generate"],
      "inputs": ["src/**/*.tsx", "src/**/*.ts"]
    },
    "lint": {
      "outputs": []
    },
    "dev":{
      "dependsOn": ["db:generate"],
      "cache": false
    },
    "db:generate": {
      "cache": false
    }
  }
}
