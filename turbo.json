{
  "pipeline": {
    "build": {
      "outputs": ["dist/**", "build/**"],
      "dependsOn": ["^build", "api#db:generate", "db:postgenerate"],
      "inputs": ["**/**/*.tsx", "**/**/*.ts"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "dependsOn": ["db:generate"],
      "cache": false
    },
    "db:generate": {
      "cache": false
    },
    "db:postgenerate": {
      "cache": false
    }
  }
}
